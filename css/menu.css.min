body{font-family:Arial,Helvetica,sans-serif}*{box-sizing:border-box}.hidden>div{display:none}.visible>div{display:block}.chat-header{width:250px;height:35px;background:#8bc34a;line-height:33px;text-indent:20px;border:1px solid #777;border-bottom:0}.chat-content{width:250px;height:300px;background:#fff;border:1px solid #777;overflow-y:auto;word-wrap:break-word}.box{width:10px;height:10px;background:green;float:left;position:relative;top:11px;left:10px;border:1px solid #ededed}.chat-box{position:fixed;right:15px;bottom:0;box-shadow:0 0 .1em #000}.chat-closed{width:250px;height:35px;background:#8bc34a;line-height:35px;font-size:18px;text-align:center;border:1px solid #777;color:#000}.butto_down{position:relative;bottom:20px;left:5px;right:5px;top:250px}.chat-content{width:250px;height:300px;background:#fff;border:1px solid #777;overflow-y:auto;word-wrap:break-word}